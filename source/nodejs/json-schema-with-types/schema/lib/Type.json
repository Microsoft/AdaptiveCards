{
  "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/andrewleader/specing-with-types/source/nodejs/json-schema-with-types/schema/lib/Type.json",
  "id": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "object",
  "description": "Expresses a typed object",
  "properties": {
    "type": {
      "type": "string",
      "description": "The type of the object"
    },
    "description": {
      "type": "string",
      "description": "A description of the object"
    },
    "properties": {

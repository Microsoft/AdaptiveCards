{
  "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/andrewleader/specing-with-types/source/nodejs/json-schema-with-types/schema/lib/Type.json",
  "type": "Type",
  "description": "Expresses a typed object",
  "properties": {
    "type": {
      "type": "string",
      "description": "The type of the object",
      "required": true
    },
    "description": {
      "type": "string",
      "description": "A description of the object"
    },
    "properties": {
      "type": "Dictionary<Property>",
      "description": "Properties of the object"
    },
    "isAbstract": {
      "type": "boolean",
      "description": "Specifies whether this type is abstract"
    },
    "extends": {
      "type": "string",
      "description": "Type that this type extends from"
    },
    "$schema": {
      "type": "uri",
      "description": "JSON schema for the JSON file"
    }
  }
}
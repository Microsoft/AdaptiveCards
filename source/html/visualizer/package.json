{
  "name": "adaptive-cards-visualizer",
  "version": "0.1.0",
  "description": "Adaptive Card Visualizer for HTML renderer",
  "main": "built/visualizer.js",
  "private": true,
  "scripts": {
    "force-update": "npm install adaptive-cards -f",
    "build": "webpack",
    "build-verbose": "webpack --display-error-details",
    "prebuild-css": "node-sass ./scss/ -o ./css --output-style expanded",
    "build-css": "postcss ./css/*.css --use autoprefixer --replace --no-map",
    "watch": "webpack --watch",
    "start": "http-server -c-1 -o -p 5666",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/ace": "0.0.32",
    "@types/markdown-it": "0.0.1",
    "@types/node": "0.0.2",
    "@types/vkbeautify": "^0.99.0",
    "ajv-cli": "1.1.1",
    "autoprefixer": "^6.7.7",
    "brace": "^0.9.1",
    "http-server": "0.9.0",
    "json-loader": "0.5.4",
    "markdown-it": "^8.2.2",
    "microsoft-adaptivecards": "^0.5.4",
    "node-sass": "^4.5.2",
    "postcss": "^5.2.16",
    "postcss-cli": "^3.1.1",
    "ts-loader": "^2.0.3",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5",
    "vkbeautify": "^0.99.1",
    "webpack": "^2.3.1"
  },
  "author": "Microsoft",
  "license": "MIT"
}

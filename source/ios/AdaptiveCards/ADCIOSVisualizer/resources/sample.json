    "emphasis": {
      "backgroundColor": "#08000000",
      "fontColors": {
        "default": {
          "normal": "#333333",
          "subtle": "#EE333333"
        },
        "accent": {
          "normal": "#2E89FC",
          "subtle": "#882E89FC"
        },
        "attention": {
          "normal": "#cc3300",
          "subtle": "#DDcc3300"

      "classType": "Enum",
      "values": [
        "cover",
        "repeatHorizontally",
        "repeatVertically",
        "repeat"
      ],
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "enum": [
            "ImageFillMode"
          ],
          "description": "Must be `ImageFillMode`"
        }
      }

    "ImageSize": {
      "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/c5318f460c665a02ea0482f114c4afb7d8acbee6/source/nodejs/json-schema-with-types/schema/lib/Type.json",
      "classType": "Enum",
      "description": "Controls the approximate size of the image. The physical dimensions will vary per host. Specify `\"auto\"` for true image dimension, or `\"stretch\"` to force it to fill the container.",
      "values": [
        "auto",
        "stretch",
        "small",
        "medium",
        "large"
      ],
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "enum": [
            "ImageSize"
          ],
          "description": "Must be `ImageSize`"
        }
      }
    },
    "ImageStyle": {
      "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/c5318f460c665a02ea0482f114c4afb7d8acbee6/source/nodejs/json-schema-with-types/schema/lib/Type.json",
      "classType": "Enum",
      "description": "Controls how this `Image` is displayed.",
      "values": [
        "default",
        "person"
      ],
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "enum": [
            "ImageStyle"
          ],
          "description": "Must be `ImageStyle`"
        }
      }
    },
    "Spacing": {
      "$schema": "https://raw.githubusercontent.com/microsoft/AdaptiveCards/c5318f460c665a02ea0482f114c4afb7d8acbee6/source/nodejs/json-schema-with-types/schema/lib/Type.json",
      "classType": "Enum",
      "description": "Specifies how much spacing. Hosts pick the exact pixel amounts for each of these.",
      "values": [
        "default",
        "none",
        "small",
        "medium",
        "large",
        "extraLarge",
        "padding"

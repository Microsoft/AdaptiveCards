{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveCard",
	"version": "1.2",
	"body": [
		{
			"type": "TextBlock",
			"text": "This card has two actions, one of which has an icon. The icon is always placed to the left of title.",
			"wrap": true
		}
	],
	"actions": [
		{
			"type": "Action.OpenUrl",
			"sentiment": "default",
			"url": "http://adaptivecards.io",
			"title": "Action.Default"
		},
		{
			"type": "Action.OpenUrl",
			"sentiment": "positive",
			"url": "http://adaptivecards.io",
			"title": "Action.Pawsitive"
		},
		{
			"type": "Action.OpenUrl",
			"sentiment": "destructive",
			"url": "http://adaptivecards.io",
			"title": "Action.Destructive"
		}
	]
}